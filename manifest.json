{
  "name": "YaaW",
  "description": "Yet Another Aria2 Web frontend for Chrome",
  "version": "0.1.3",
  "default_locale": "en",
  "background_page": "background.html",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16" : "img/aria-16.png",
    "24" : "img/aria-24.png",
    "128": "img/aria-128.png"
  },
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "notifications",
    "contextMenus",
    "tabs",
    "cookies"
  ]
}
